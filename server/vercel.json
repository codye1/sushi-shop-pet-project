{
    "version":2,
    "builds":[
        {
            "src":"*.js",
            "use":"@vercel/node",
            "config":{
                "includeFiles":["db.json"]
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}